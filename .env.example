# Firebase Configuration
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_DATABASE_URL=https://your-project-default-rtdb.firebaseio.com/

# n8n Configuration
N8N_WEBHOOK_URL=http://localhost:5678/webhook/
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# Development Settings
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# ESP32 Configuration (for reference)
WIFI_SSID=your-wifi-network
WIFI_PASSWORD=your-wifi-password
FIREBASE_HOST=your-project-default-rtdb.firebaseio.com
FIREBASE_AUTH=your-firebase-database-secret

# GSM Configuration
GSM_APN=your-gsm-apn
GSM_USER=your-gsm-username
GSM_PASS=your-gsm-password

# PZEM Configuration
PZEM_ADDRESS=0x01
PZEM_BAUD_RATE=9600

# Mobile App Configuration
GOOGLE_SERVICES_JSON_PATH=./app/google-services.json

# Optional: Redis Configuration
REDIS_URL=redis://localhost:6379

# Optional: Monitoring
PROMETHEUS_ENABLED=false
GRAFANA_ENABLED=false